<section class="speaker-list">
  <% headline, regular = blog('speakers').articles.partition {|s| s.data.try :headline } %>

  <% alphabetize(headline).each do |speaker| %>
    <%= partial "partials/speaker", locals: { speaker: speaker } %>
  <% end %>

  <% alphabetize(regular).each do |speaker| %>
    <%= partial "partials/speaker", locals: { speaker: speaker } %>
  <% end %>
</section>
